  {
    "name": "StartTab",
    "version": "1.0",
    "description": "Here is our new start tab!",
    "permissions": [
      "tabs",
      "activeTab",
      "declarativeContent",
      "storage",
      "history",
      "bookmarks"
    ],
    "background": {
      "page": "background.html",
      "persistent": false
    },
    "browser_action": {
      "default_title": "StartTab",
      "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "commands": {
      "store-tab": {
        "suggested_key": {
          "default": "Alt+C",
          "mac": "Alt+C"
        },
        "description": "Store tab"
      },
      "store-tabs": {
        "suggested_key": {
          "default": "Ctrl+K",
          "mac": "Command+K"
        },
        "description": "Store tabs"
      }
    },
    "chrome_url_overrides" : {
      "newtab": "start.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "css": ["styles/main.css"],
        "js": ["content/main.js"]
      }
    ],
    "content_security_policy": "script-src 'self' https://cdn.bootcdn.net; object-src 'self'",
    "manifest_version": 2
  }
